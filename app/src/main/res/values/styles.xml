<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="LoginTheme">
        <item name="android:windowBackground">@color/white</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="rounded_image" parent="">
        <item name="strokeWidth">20dp</item>
        <item name="backgroundColor">@color/dark_teal</item>
        <item name="cornerSize">50%</item>
        <item name="android:background">@color/white</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="Shape_post_buttons" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
        <item name="cornerSizeTopLeft">50%</item>
        <item name="cornerSizeTopRight">50%</item>
        <item name="cornerSizeBottomRight">-50%</item>
    </style>

    <style name="rounded_corners_image" parent="">
        <item name="strokeWidth">20dp</item>
        <item name="backgroundColor">@color/dark_teal</item>
        <item name="cornerSize">3%</item>
        <item name="android:background">@color/white</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="Rounded" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>

    <style name="Widget.Theme.ConnectDot.ActionBar.Fullscreen" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>

    <style name="Widget.Theme.ConnectDot.ButtonBar.Fullscreen" parent="">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
    </style>

</resources>